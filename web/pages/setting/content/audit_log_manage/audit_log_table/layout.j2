<div class="my-table no-context-menu w850">
    <div id="place-for-audit-log-table">

        <table
            id                  = "audit-log-table"
            class               = "table-sm clickable"
            data-toggle         = "table"
            data-sortable       = "false"
            data-click-to-select= "true"
            data-row-style      = "redRowStyle"
        >
            <thead>
                <tr>
                    <th data-field="state" data-radio="true"></th>
                    <th data-field="log_time"     data-cell-style="fitCenterCellStyle">{{ textData.AUDIT_LOG_MANAGE_LOG_TIME }}</th>
                    <th data-field="ipv4"         data-cell-style="fitCenterCellStyle">{{ textData.IP                        }}</th>
                    <th data-field="log_user"     data-cell-style="fitCenterCellStyle">{{ textData.USER                      }}</th>
                    <th data-field="log_category" data-cell-style="fitCenterCellStyle">{{ textData.CATEGORY                  }}</th>
                    <th data-field="log_type"     data-cell-style="fitCenterCellStyle">{{ textData.TYPES                     }}</th>
                    <th data-field="content"      data-cell-style="fitCenterCellStyle">{{ textData.ADDITIONAL_INFO           }}</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>

    </div>
</div>